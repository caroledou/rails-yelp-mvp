<h1>Edit - <%= @restaurant.name %></h1>

<%= simple_form_for [@restaurant] do |f| %>
  <%= f.input :name %>
  <%= f.input :address %>
  <%= f.input :phone_number %>
  <%= f.input :category %>

  <%= f.submit "Edit restaurant", class: "btn btn-primary" %>
<% end %>

<%= link_to "Back", admin_restaurants_path  %>


